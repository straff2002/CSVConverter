        <script type="text/javascript">
        $(document).ready(function() {
            $("#conf_method").change(function(event) {
                var meth = $("#conf_method").val();
                if (meth === "upload"){
                $(".sel-a").show();
                $(".sel-b").hide();
                                        }
                else {
                $(".sel-a").hide();
                $(".sel-b").show();
                      }
                                                       }
                          )}
                          );
         function addFile() {
            $(".P5").toggle() }
        function hideFunc(el) {
            if ( el == 'csv' ){
                $(".converter").show();
                $(".downloader").hide();}
            else {
                $(".downloader").show();
                $(".converter").hide();}}
        </script>
        <script type="text/javascript">
        $(document).ready(function() {
        $("#search_conf").keyup(function(){
            var data = $("#search_conf").val();
            $.post("/conv/getconf.py", {"confname": data}, function(data, status){$("#data_conf").html(data)}
                   );
                                          })
                                         });

        </script>
        <script type="text/javascript">
        $(document).ready(function() {
        $("#search_conf").change(function(event){
            var data = $("#search_conf").val();
            $.post("/conv/getprev.py", {"confname": data}, function(data, status){$("#preview").html(data)}
                   );
                                          })
                                       });
        </script>
